{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/visa_oracle/examples.schema.json",
  "title": "Visa Oracle Example",
  "type": "object",
  "required": [
    "id",
    "visa_code",
    "scenario",
    "expected"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "visa_code": {
      "type": "string"
    },
    "scenario": {
      "type": "object",
      "properties": {
        "nationality": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        },
        "purpose": {
          "type": "string"
        },
        "stay_length_days": {
          "type": "integer"
        },
        "in_indonesia_now": {
          "type": "boolean"
        },
        "has_indonesian_spouse": {
          "type": "boolean"
        },
        "sponsor": {
          "type": "string",
          "description": "employer | spouse | institution | none"
        },
        "remote_work_for_overseas_company": {
          "type": "boolean"
        },
        "intends_to_work_in_indonesia": {
          "type": "boolean"
        },
        "has_job_offer_in_indonesia": {
          "type": "boolean"
        },
        "education_offer": {
          "type": "boolean"
        },
        "investment_budget_usd": {
          "type": "number"
        },
        "monthly_income_usd": {
          "type": "number"
        },
        "liquid_assets_usd": {
          "type": "number"
        },
        "property_value_usd": {
          "type": "number"
        },
        "dependents_count": {
          "type": "integer"
        },
        "notes": {
          "type": "string"
        }
      }
    },
    "expected": {
      "type": "object",
      "required": [
        "recommended_visa",
        "eligibility",
        "key_reasons"
      ],
      "properties": {
        "recommended_visa": {
          "type": "string"
        },
        "eligibility": {
          "type": "string",
          "enum": [
            "eligible",
            "ineligible",
            "conditional"
          ]
        },
        "key_reasons": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "estimated_cost_breakdown": {
          "type": "object"
        },
        "timeline_weeks": {
          "type": "string"
        },
        "key_requirements": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "warnings": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "citations": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}

