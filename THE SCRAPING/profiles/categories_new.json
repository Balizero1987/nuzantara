      "extraction_schema": {
        "required_fields": ["regulation_number", "effective_date", "what_changed", "impact_on", "compliance_deadline"],
        "optional_fields": ["penalties", "transition_period", "supersedes_regulation"],
        "validation_rules": {
          "regulation_number": "^(UU|PP|Perpres|PMK|Permen|Permenkumham)\\s+No\\.?\\s+\\d+\\s+Tahun\\s+\\d{4}$",
          "effective_date": "^\\d{2}/\\d{2}/\\d{4}$",
          "impact_on": ["visa", "tax", "business_license", "property", "labor", "banking"]
        }
      },
      "llama_prompt": "Extract REGULATORY CHANGE information:\n\nREQUIRED:\n1. Regulation: Full number (e.g., 'Perpres No. 10 Tahun 2024')\n2. Effective Date: DD/MM/YYYY\n3. What Changed: Specific changes vs previous regulation\n4. Impact On: [Visa holders, PT/PT PMA owners, Property owners, Tax obligations]\n5. Compliance Deadline: When must clients act\n6. Penalties: Fines/consequences of non-compliance\n\nOUTPUT:\nIf regulation impacts our clients → Extract structured data\nIf general/non-relevant → SKIP\n\nCRITICAL: Only regulations affecting foreign businesses/individuals",
      "content_output": {
        "email_template": "regulatory_alert",
        "blog_template": "regulation_explainer",
        "social_template": "regulation_announcement"
      },
      "success_metrics": {
        "alert_speed": "< 24 hours from publication",
        "client_compliance": "100% (zero penalties)",
        "blog_traffic": "Top 10 for '{regulation} Indonesia'"
      }
    },
