<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.nuzantara.memory.monitor</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/antonellosiano/Desktop/nuzantara/scripts/memory_monitor.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/memory_monitor.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/memory_monitor_error.log</string>

    <key>StartInterval</key>
    <integer>30</integer>
</dict>
</plist>