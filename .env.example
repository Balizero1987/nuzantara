# Zantara Multi-Agent Configuration
# Copy questo file come .env e compila i valori

# API Authentication
X_API_KEY=your_api_key_here
X_BZ_USER=BOSS

# Service Configuration
BASE_URL=https://zantara-bridge-v2-prod-1064094238013.asia-southeast2.run.app
MONTHLY_LIMIT=100

# Google Cloud
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json
 
# Google Workspace / Calendar
# Default calendar to use for calendar.create/get/list when not provided explicitly
ZANTARA_CALENDAR_ID=c_7000dd5c02a3819af0774ad34d76379c506928057eff5e6540d662073aaeaaa7@group.calendar.google.com

# Google Maps (set one of these)
# GOOGLE_MAPS_API_KEY=your_maps_api_key
# GOOGLE_MAPS_KEY=your_maps_api_key
# GOOGLE_API_KEY=your_maps_api_key

# Agent Limits
MAX_EXECUTION_TIME=300
MAX_MEMORY_MB=512
AGENT_TIMEOUT=60

# Logging
LOG_LEVEL=INFO
LOG_TO_CLOUD=true

# Development
DEBUG_MODE=false
MOCK_EXTERNAL_APIS=false
