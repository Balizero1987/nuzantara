{
  "name": "@nuzantara/webapp",
  "version": "5.2.0",
  "type": "module",
  "description": "NUZANTARA Web Application",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || true",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix || true",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" || true",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" || true",
    "clean": "find js -name '*.js' -type f -exec sed -i '' 's|https://nuzantara-[a-z-]*\\.fly\\.dev|window.API_CONFIG?.backend?.url || \\'/api\\'|g' {} \\; || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lucide-react": "^0.555.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@swc/core": "^1.15.2",
    "@swc/jest": "^0.2.39",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "terser": "^5.44.1",
    "typescript": "^5.2.0",
    "vite": "^7.2.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
