{
  "name": "zantara",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "bench": "tsx benchmarks/bench.ts",
    "bench:memory": "tsx benchmarks/memory-bench.ts",
    "migrate": "tsx scripts/migrate-routes.ts",
    "migrate:memory": "tsx scripts/migrate-memory-data.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "redis": "^4.7.0",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/autocannon": "^7.9.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autocannon": "^7.14.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
